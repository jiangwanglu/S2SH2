<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
                 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
        
<hibernate-mapping>
	<class name="com.pp.test.bo.MaintenancePlan" table="MaintenancePlan">
		<id name="Number" column="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="unitid" />
		<property name="data" />
		<property name="category" />
		<property name="name" />
		<property name="running" />
		<property name="executioncycle" />
		<property name="executor" />
		<property name="mechanism" />
	</class>
</hibernate-mapping>